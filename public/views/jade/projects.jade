extends partials/layout

block content
	.main-content
		h1.glyphicon.glyphicon-road #{title}
		br
		br
		h2.text-center.jumbotron  #{title} List
		table.table.table-bordered.table-striped(ng-controller="ProjectListController")
			thead: tr
				th Name
				th Description
			tboday
				tr.success(ng-repeat="project in projects")
					td {{ project.name }}
					td {{ project.description }}
